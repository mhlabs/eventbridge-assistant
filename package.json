{
  "name": "eventbridge-assistant",
  "displayName": "EventBridge Assistant",
  "description": "Autocomplete for building patterns against EventBridge schemas",
  "version": "0.0.8",
  "publisher": "ljacobsson",
  "engines": {
    "vscode": "^1.63.0"
  },
  "categories": [
    "Other"
  ],  
	"icon": "images/icon.png",
  "keywords": [
    "yaml",
    "eventbridge",
    "aws",
    "serverless"
  ],
  "repository": {
		"type": "git",
		"url": "https://github.com/mhlabs/eventbridge-assistant.git"
	},
  "activationEvents": [
    "onLanguage:yaml"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/jsonpath": "^0.2.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.63.0",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "@vscode/test-electron": "^2.0.3",
    "eslint": "^8.6.0",
    "glob": "^7.2.0",
    "mocha": "^9.1.3",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@mhlabs/aws-sdk-sso": "^0.0.16",
    "aws-sdk": "^2.1060.0",
    "json-find": "^2.0.8",
    "jsondiffpatch": "^0.4.1",
    "jsonpath": "^1.1.1",
    "yaml-cfn": "^0.3.1"
  }
}
